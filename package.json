{
  "name": "mafia-moderator",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "dexie": "^2.0.4",
    "framework7": "^3.5.2",
    "framework7-icons": "^2.0.3",
    "framework7-react": "^3.5.2",
    "material-design-icons": "3.0.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
